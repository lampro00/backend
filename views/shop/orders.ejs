<%- include('../includes/head.ejs') %>
    </head>

    <body>
        <%- include('../includes/navigation.ejs') %>
        <main>
<% if (orders.length <=0){ %> 

            <h1>Nothing there!</h1>
            <% } else { %>
                <ul>
                    <% orders.forEach(order=>{ %>
                         <li>
                            <h1># <%=order._id %></h1>
                            <ul>
                                 <% order.product.forEach(products=>{ %>
                                     <li><%=products.product.title %> (<%=products.quantity %>)</li>
                                  <% }); %>
                            </ul>
                          </li>
                     <% }); %>
                </ul>
            <% } %>
        </main>
        <%- include('../includes/end.ejs') %>